    "@typescript-eslint/parser": "8.37.0",
    "dotenv": "17.2.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-json": "^4.0.1",
    "jest": "30.0.4",
    "jest-environment-jsdom": "30.0.4",
    "nx": "21.2.3",
    "nx-cloud": "19.1.0",
