    "@angular-devkit/schematics": "20.1.0",
    "@commitlint/cli": "19.8.1",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "~9.31.0",
    "@nx/devkit": "21.2.3",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "21.2.3",
