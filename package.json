    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.11.0"
  },
  "files": [
    "src/*",
    "cli/*",
    "lib/*",
    "peers/*",
    "scripts/*.js",
    "stubs/*.stub.js",
    "nesting/*",
    "types/**/*",
    "*.d.ts",
    "*.css",
    "*.js"
